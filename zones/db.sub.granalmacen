; Archivo de zona sub.granalmacen.int
$TTL 1m
@       IN      SOA     ns1.sub.granalmacen.int. admin.granalmacen.ib. (
                        2008052001 ; Serial
                        2m         ; Refresh
                        1m         ; Retry
                        1d         ; Expire
                        1m )       ; Negative Cache TTL

; Servidores DNS de la Subzona (NS)
@       IN      NS      ns1.sub.granalmacen.int.
@       IN      NS      ns2.sub.granalmacen.int.

; Registros A de los servidores NS
ns1     IN      A       172.16.8.10   ; Servidor Docker
ns2     IN      A       172.16.8.11   ; Secundario ficticio

; Estaciones de Trabajo
pc1     IN      A       172.16.8.50   ; 
pc2     IN      A       172.16.8.51   ;
pc3     IN      A       172.16.8.52   ; 
pc4     IN      A       172.16.8.53   ; 
pc5     IN      A       172.16.8.54   ; 